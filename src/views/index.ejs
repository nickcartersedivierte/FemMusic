 <div class = "container p-5">
    <div class= "row">
    <div class = ".col-md-4 offset-md-4">

     <form action = "/create" method = "POST" class ="card card-body">
     <div class = "form-group">
      <input type = "text" name = "Name" placeholder ="Name" class= "form-control" autofocus>
      </div>

      <div class = "form-group">
      <input type = "text" name = "Instrument" placeholder ="Instrument" class= "form-control">
      </div>

      <button class ="btn btn-primary">
        Submit 
      </button>
    </form>
   
 

 <% musicians.forEach(function (musician) { %>
    <div class = "card.card-body mt-2">
    <h5><%= musician.Name %></h5>
    <h5><%= musician.Instrument %></h5>
    </div>
<% }) %>
        </div>
    </div>
 </div>

