 <!-- Home Content -->
  <div class="container">
    <div class="row">
      <div class="col-lg-12 text-center">
        <h1 class="mt-5">FemMusic</h1>
        <p class="lead"> A web dedicated to women in Music </p>
        <div class = "imgshome">
        <img src = "img/music.jpg" alt = "Guitar" width= "270" height="250"> </img>
        <br>
        </div>
